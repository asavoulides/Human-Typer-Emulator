{
  "manifest_version": 2,
  "name": "Human Typing Emulator",
  "version": "1.0",
  "description": "Emulates human typing in Google Docs",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
